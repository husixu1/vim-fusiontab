{
  "name": "coc-exposejump",
  "version": "0.0.0-development",
  "description": "Expose some internal states of coc.nvim.",
  "author": "Sixu Hu <husixu1@hotmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "coc.nvim": "^0.0.79",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-exposejump configuration",
      "properties": {
        "coc-exposejump.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-exposejump extension"
        }
      }
    },
    "commands": [
      {
        "command": "coc-exposejump.Command",
        "title": "coc-exposejump command title"
      }
    ]
  }
}
